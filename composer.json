{
    "name": "jq/cray",
    "description": "A disposable Laravel package to help you generate nearly complete CRUD pages like crazy!",
    "keywords": [
        "junaidqadirb",
        "cray",
        "scaffold",
        "generate",
        "generate code",
        "code generator"
    ],
    "homepage": "https://github.com/junaidqadirb/cray",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Junaid Qadir",
            "email": "junaidqadirb@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "doctrine/dbal": "^3.3.4",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0|^10"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^4.0|^5.0|v6.0.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JunaidQadirB\\Cray\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JunaidQadirB\\Cray\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JunaidQadirB\\Cray\\CrayServiceProvider"
            ],
            "aliases": {
                "Cray": "JunaidQadirB\\Cray\\CrayFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
